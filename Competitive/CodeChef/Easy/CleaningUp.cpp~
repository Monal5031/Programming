#include"bits/stdc++.h"
using namespace std;
int main()
{
ios::sync_with_stdio(false);
int tc;
cin>>tc;
while(tc--)
	{
	int n,m;
	cin>>n>>m;
	if(n==m)
	cout<<" \n \n";
	else
		{
		bool arr[n+1]; int tmp;
		
		for(int i=0;i<m;i++)
			{
			cin>>tmp;
			arr[tmp]=true;
			}
		vector <int>chef;
		vector <int>assis;
		for(int i=1,k=1;i<n+1;i++)
			if(arr[i]==true)
				{
				if(k%2==1)
					chef.push_back(i);
				else
					assis.push_back(i);
				k++;
				}
		for(int i=0;i<chef.size();i++)
			cout<<chef[i]<<" ";
		cout<<"\n";
		for(int i=0;i<assis.size();i++)
			cout<<assis[i]<<" ";
		cout<<"\n";		
		}
	}
return 0;
}
